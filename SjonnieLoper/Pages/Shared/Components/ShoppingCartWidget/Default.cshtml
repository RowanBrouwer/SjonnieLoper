@model List<ShoppingCartItem>

@if (Model.Count > 0)
{
    <li class="nav-item">
        <a class="nav-link text-dark" 
           asp-page="/ShoppingCartPages/Index">
            <span id="cart-status">
                <i class="fas fa-shopping-cart"></i>
                @Model.Count.ToString()
            </span>
        </a>
    </li>
}